{
  "name": "dashboard",
  "author": "Bogdan Bota",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@material-ui/core": "^3.9.1",
    "@material-ui/icons": "^3.0.2",
    "cross-env": "5.1.6",
    "electron-is-dev": "^1.0.1",
    "electron-log": "^2.2.6",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.0",
    "strip-ansi": "^5.2.0",
    "update-electron-app": "^1.2.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.0.2",
    "electron-builder": "^20.38.5",
    "prettier": "^1.4.4",
    "react-scripts": "2.1.3",
    "wait-on": "^3.2.0"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "cross-env BROWSER=none react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "electron-build": "electron-builder",
    "electron": "electron .",
    "publish": "cross-env CI=true react-scripts test && yarn react-build && yarn electron-build --publish=always",
    "build": "cross-env CI=true react-scripts test && yarn react-build && yarn electron-build",
    "start": "concurrently \"yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "react-start-web": "cross-env REACT_APP_TARGET=web PORT=3001 react-scripts start",
    "react-build-web": "cross-env REACT_APP_TARGET=web react-scripts build"
  },
  "build": {
    "publish": {
      "provider": "github"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
