  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "author": "Bogdan Bota",
  "dependencies": {
    "@material-ui/core": "^3.9.1",
    "@material-ui/icons": "^3.0.2",
    "electron-is-dev": "^1.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
